classdef Point(x, y)

def Point_print(point, name):
  print(name, " (", point.x, " ", point.y, ")")
  
def Point_equals(point1, point2):
  point1.x == point2.x ?(
    point1.y == point2.y ?(
      true
    |
      false
    )
  |
    false
  )
  
def main():
  p = Point(1, 2)
  p2 = Point { y = 2, x = 1 }
  
  Point_print(p, "p")
  Point_print(p2, "p2")
  
  Point.print = Point_print
  p.print("p")
  p2.print("p2")
 
 